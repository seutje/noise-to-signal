{
  "args": {
    "data_root": "data/sd15_abstract",
    "manifest": "data/sd15_abstract/manifest.csv",
    "output": "training/outputs",
    "resolution": 256,
    "batch_size": 4,
    "num_workers": 4,
    "epochs": 100,
    "val_split": 0.05,
    "seed": 42,
    "generator_lr": 1e-05,
    "discriminator_lr": 1e-05,
    "weight_decay": 0.0001,
    "latent_channels": 8,
    "ema_decay": 0.999,
    "no_ema": false,
    "resume": null,
    "override_manifest_limit": null,
    "no_grad_checkpoint": false,
    "precision": "16-mixed",
    "grad_clip": 1.0,
    "checkpoint_every": 1,
    "devices": 1,
    "strategy": "auto",
    "accumulate_grad_batches": 1,
    "num_samples": 8,
    "latent_sigma": 1.0,
    "r1_gamma": 10.0,
    "r1_interval": 16,
    "chroma_weight": 1.5,
    "saturation_weight": 0.75,
    "sample_interval": 400
  }
}